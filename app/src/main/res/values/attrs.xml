<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleView">
        <attr name="title" format="string"/>
        <attr name="rightText" format="string"/>
        <attr name="backIcon" format="reference"/>
        <attr name="hideBackIcon" format="boolean"/>
        <attr name="rightIcon" format="reference"/>
        <attr name="showRightIcon" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="LabelTextView">
        <attr name="labelText" format="string"/>
        <attr name="symbolText" format="string"/>
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="size" format="dimension"/>
        <attr name="labelSize" format="dimension"/>
        <attr name="textMarginLeft" format="dimension"/>
        <attr name="symbolTextMarginLeft" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="color" format="color"/>
        <attr name="labelColor" format="color"/>
    </declare-styleable>
    <attr name="app_primary_color" format="color"/> <!-- topbar -->
    <attr name="app_content_bg_color" format="color"/> <!-- content-->
</resources>